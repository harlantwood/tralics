  DocType = rr raweb.dtd 
BeginTitlePage
  \makeRR <RRstart> "" "type = 'RR'"
  alias \makeRT "" "type = 'RT'"

  <UR> -
  \URSophia ?+<UR>
  \URRocquencourt ?+<UR>
  alias \URRocq
  \Paris ?<UR> <Rocquencourt>
  \URRhoneAlpes ?+<UR>
  \URRennes ?+<UR>
  \URLorraine ?+<UR>
  \URFuturs ?+<UR>

  \RRtitle q<title> "pas de titre"
  \RRetitle q<etitle>  "no title"
  \RRprojet <projet> "pas de projet"
  \motcle <motcle> "pas de motcle"
  \keyword <keyword>  "no keywords"
  \RRresume p<resume> "pas de resume"
  \RRabstract p<abstract> "no abstract"
  \RRauthor + <author> <auth> "Pas d'auteurs"
  \RRdate <date> "\monthyearvalfr"
  \RRdater <dater> "\monthyearvalfr"
  \RRversion <RRversion> "0"
  \RRNo <RRnumber> "????"
 
  \RRtheme <>  +"pas de theme" % CES
  <Theme> -                    % E
  \THNum ?+<Theme>             % CE
  \THCog ?+<Theme>             % CE
  \THCom ?+<Theme>             % CE
  \THBio ?+<Theme>             % CE
  \THSym ?+<Theme>             % CE
 
